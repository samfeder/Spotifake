<h1> Add a band | Spotifake</h1>

<form action="<%= bands_url %>" method="post">
  <input
  name="_method"
  type="hidden"
  value="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

<label for="band_name">
  Band Name
  <input type="text"
         name="band[name]"
         id="band_name">
</label>
<input type="submit">
</form>
